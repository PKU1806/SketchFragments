# 658 "p4src/ecmp.p4"
control MyComputeChecksum(inout headers hdr, inout metadata meta)
{
    apply
    {
        update_checksum(
            hdr.ipv4.isValid(),
            {hdr.ipv4.version,
             hdr.ipv4.ihl,
             hdr.ipv4.dscp,
             hdr.ipv4.ecn,
             hdr.ipv4.totalLen,
             hdr.ipv4.identification,
             hdr.ipv4.flags,
             hdr.ipv4.fragOffset,
             hdr.ipv4.ttl,
             hdr.ipv4.protocol,
             hdr.ipv4.srcAddr,
             hdr.ipv4.dstAddr},
            hdr.ipv4.hdrChecksum,
            HashAlgorithm.csum16);

  update_checksum_with_payload(
   hdr.MIH.isValid() && !hdr.SFH.isValid(),
   {hdr.ipv4.srcAddr,
    hdr.ipv4.dstAddr,
    8w0,
    hdr.ipv4.protocol,
    hdr.udp.length,
    hdr.udp.srcPort,
    hdr.udp.dstPort,
    hdr.udp.length,
    hdr.MIH},
   hdr.udp.checksum,
   HashAlgorithm.csum16);

  update_checksum_with_payload(
   hdr.MIH.isValid() && hdr.SFH.isValid(),
   {hdr.ipv4.srcAddr,
    hdr.ipv4.dstAddr,
    8w0,
    hdr.ipv4.protocol,
    hdr.udp.length,
    hdr.udp.srcPort,
    hdr.udp.dstPort,
    hdr.udp.length,
    hdr.MIH,
    hdr.SFH},
   hdr.udp.checksum,
   HashAlgorithm.csum16);
    }
}

