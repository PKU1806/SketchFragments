# 97 "p4src/ecmp.p4"
control MyIngress(inout headers hdr,
                  inout metadata meta,
                  inout standard_metadata_t standard_metadata)
{

    //get the bringable bucket index 
    action predispose(){
  meta.SFH_index = 3 * 64;

  random(meta.counter_index0, (bit<32>)0, (bit<32>)(3 * 64 - 1));
        counter0.read(meta.counter_value0,meta.counter_index1);
  if (meta.SFH_index >= 3 * 64 && meta.counter_value0 == 0) {
   meta.SFH_index = meta.counter_index0;
   meta.counter_value0 = 1;
  }
  counter0.write(meta.counter_index0, meta.counter_value0);

        random(meta.counter_index1, (bit<32>)0, (bit<32>)(3 * 64 - 1));
        counter1.read(meta.counter_value1, meta.counter_index1);
  if (meta.SFH_index >= 3 * 64 && meta.counter_value1 == 0) {
   meta.SFH_index = meta.counter_index1;
   meta.counter_value1 = 1;
  }
  counter1.write(meta.counter_index1, meta.counter_value1);

        random(meta.counter_index2, (bit<32>)0, (bit<32>)(3 * 64 - 1));
        counter2.read(meta.counter_value2, meta.counter_index2);
  if (meta.SFH_index >= 3 * 64 && meta.counter_value2 == 0) {
   meta.SFH_index = meta.counter_index2;
   meta.counter_value2 = 1;
  }
  counter2.write(meta.counter_index2, meta.counter_value2);

    }

    action _choose_fragment(bit<8> SFH_target_array){
  meta.SFH_target_bucket = meta.SFH_index -
   (bit<32>)meta.SFH_target_array * 64;
        meta.SFH_target_array = SFH_target_array + (1 - meta.sketch_fg) * 3;
    }

    table choose_fragment{
        key={
            meta.SFH_index:range;
        }
        actions={
            NoAction;
            _choose_fragment;
        }
        size=256;
        default_action=NoAction;
    }

    action update_using_sketch0(){

       array0.read(meta.tmp00,meta.SFH_target_bucket*10 +0);
       array0.read(meta.tmp01,meta.SFH_target_bucket*10 +1);
       array0.read(meta.tmp02,meta.SFH_target_bucket*10 +2);
       array0.read(meta.tmp03,meta.SFH_target_bucket*10 +3);
       array0.read(meta.tmp04,meta.SFH_target_bucket*10 +4);
       array0.read(meta.tmp05,meta.SFH_target_bucket*10 +5);
       array0.read(meta.tmp06,meta.SFH_target_bucket*10 +6);
       array0.read(meta.tmp07,meta.SFH_target_bucket*10 +7);
       array0.read(meta.tmp08,meta.SFH_target_bucket*10 +8);
